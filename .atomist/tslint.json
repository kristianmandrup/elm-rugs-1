{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "ordered-imports": [true, {
            "import-sources-order": "any",
            "named-imports-order": "case-insensitive"
        }],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "no-console": false
    },
    "rulesDirectory": []
}
